#include<iostream>
using namespace std;
int b[10000009];
int main(){
int t;
cin>>t;
while(t--){
	int n;
	cin>>n;
	int a[n][n],x=0,kt=0;
	for(int i=0;i<n;i++){
		for(int j=0;j<n;j++)cin>>a[i][j];
	}
	while(x<n){
		if(x%2==0)for(int i=0;i<n;i++)cout<<a[x][i]<<" ";
		else for(int i=n-1;i>=0;i--)cout<<a[x][i]<<" ";
		x++;
	}
cout<<endl;
}
}
