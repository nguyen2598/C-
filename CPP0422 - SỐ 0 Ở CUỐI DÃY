#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,n;
	cin>>t;
	while(t--)
	{
		cin>>n;
		long long a[n];
		int x=n-1,d=0;
		for(int i=0;i<n-d;i++){
			cin>>a[i];
			if(a[i]==0){
				a[x]=0;
				x--;
				i--;
				d++;
			}
		}
	for(int i=0;i<n;i++){
			cout<<a[i]<<" ";
		}
		cout<<endl;
	}
	// your code here

	return 0;
}
